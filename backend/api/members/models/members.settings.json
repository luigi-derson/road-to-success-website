{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "Members"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "role": {
      "type": "string",
      "required": true
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "experience": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ],
      "required": true
    },
    "achievements": {
      "type": "component",
      "repeatable": true,
      "component": "items.achievements"
    },
    "order": {
      "type": "integer",
      "required": true,
      "unique": true
    }
  }
}
