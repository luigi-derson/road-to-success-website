{
  "kind": "collectionType",
  "collectionName": "galleries",
  "info": {
    "name": "Gallery"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": false
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "date": {
      "type": "date"
    }
  }
}
